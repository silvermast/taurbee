{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPLOCALDATA/**"
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "global-shortcut:default",
    "global-shortcut:allow-is-registered",
    "global-shortcut:allow-register",
    "global-shortcut:allow-register-all",
    "global-shortcut:allow-unregister",
    "global-shortcut:allow-unregister-all",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "fs:default",
    "shell:default",
    "dialog:default",
    "os:default",
    "fs:default"
  ]
}